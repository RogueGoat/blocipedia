<h1>Blocis - Check them out!</h1>

<div class = "new-wiki">
<%= link_to "New", new_wiki_path, class: 'btn pull-right' %>
</div>

<% @wiki.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki %>
      </h4>
    </div>
  </div>
<% end %>